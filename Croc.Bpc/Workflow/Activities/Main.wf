      <ReferenceActivity Name="SayYesForNewElectionNoForBackToSummarizing" Execute="Common.SayAndWaitYesOrNo" 
                         Tracking="false" 
                         k:Yes="ResetUik" k:No="Summarizing"/> 


      <Activity Name="ResetUik" Execute="ResetUik"  
