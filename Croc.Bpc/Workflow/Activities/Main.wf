      <ReferenceActivity Name="Summarizing" Execute="SummarizingActivity" Priority="4" 
                         Parameters="Order=9"/> 


      <ReferenceActivity Name="IsMasterScanner2" Execute="Common.IsMasterScanner" Priority="4" 
                         Parameters="Order=9" 
